function Jugg::Kill(%Jugg, %Juggnum)
{
	if(%Jugg.deadJugg)
		return;

	Player::blowUp(%Jugg);
	Player::Kill(%Jugg);

	GameBase::PlaySound(%Jugg, shockExplosion, 0);
}

