function TowerSwitch::onCollision(%this, %object)
{
	if(getObjectType(%object) != "Player")
		return;

	else if(Player::isDead(%object))
		return;

	else if(GameBase::getMapName(%this) == "Single Jugg")
	{
		Jugg::Pull();
		GameBase::PlaySound(%this, SoundFireDisc, 3);
		return;
	}
	else if(GameBase::getMapName(%this) == "Auto Juggs")
	{
		if($AutoLaunchJuggs != "true")
		{
			$AutoLaunchJuggs = "true";
		}
		else
		{
			$AutoLaunchJuggs = "";
		}
		Jugg::Pull();
		GameBase::PlaySound(%this, SoundFireDisc, 3);
		return;
	}
	else
	{
	}
}

function PlasmaCannoner2::Detonate(%client, %player, %time)
{
	if(Player::isDead(%player)){return;}
	if(%player.plasmacharge != 15){return;}

	%spos = "247.674 61.7007 51.001";
	%jpos = Gamebase::getposition(%player);
	%dist = Vector::getDistance(%jpos, %spos);

	if(%time > 0)
	{
		%time--;
		schedule("PlasmaCannoner2::Detonate(" @ %client @ ", " @ %player @ ", " @ %time @ ");",1.0,%player);
	}


	if(%dist <= 175)
	{
		gamebase::setrotation(%player, Vector::FindRotation(%jpos, %spos));
		PlasmaCannonImage::onFire(%player, 0);
		if(%player.isJugg == "1")
		{
			%aiName = Client::getName(%client);
			AI::DirectiveWaypoint(%aiName, $JuggTarget, 1);
			schedule("Beacon::onUse(" @ %player @ ", beacon);", 1.05, %player);
		}
		return;
	}
}
